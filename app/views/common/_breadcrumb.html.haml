.row
  .span8
    %ul.breadcrumb
      %li
        %i.icon-home
        = link_to("Home", home_index_path)
        %span.divider
          ="/"
      - paths.each do |p|
        - if paths.last == p
          %li.active= p[:name]
        - else
          %li
            = link_to(p[:name], p[:link])
            %span.divider
              ="/"
  .span3
    = link_to("Logout",destroy_user_session_path, class: 'btn logout')